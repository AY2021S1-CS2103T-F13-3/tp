<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<HBox id="row" fx:id="row" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane>
        <Label fx:id="fieldName" styleClass="cell_small_label" text="\$fieldName" />
    </AnchorPane>
    <AnchorPane>
        <FlowPane fx:id="tags" />
    </AnchorPane>
</HBox>